@model IEnumerable<Viljaladu.Models.Auto>
@{ 
    double kogus = (double) TempData["mass"];
}

<h2>Auto Detailid</h2>

<div>
    <hr />
    <h5>Kokku toodud vili: @kogus kg.</h5>
</div>
<table class="table">
    <tr>
        <th>
            <b>Auto Number</b>
        </th>
        <th>
            <b>Sisenemismass</b>
        </th>
        <th>
            <b>Lahkumismass</b>
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.autoNr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sisenemisMass)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lahkumisMass)
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Back", "IndexAutod")
</p>